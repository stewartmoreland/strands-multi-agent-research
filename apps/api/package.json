{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/lambda.js",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild src/lambda.ts --bundle --minify --outfile=dist/lambda.js",
    "dev": "tsx watch --env-file=.env.local --import ./src/instrument.ts src/local.ts",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.30.2",
    "@aws-sdk/client-bedrock": "^3.985.0",
    "@aws-sdk/client-bedrock-agentcore": "^3.985.0",
    "@codegenie/serverless-express": "^4.17.1",
    "@middy/core": "^7.0.2",
    "@repo/shared": "*",
    "@repo/util": "*",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "supertest": "^7.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
