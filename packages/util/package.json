{
  "name": "@repo/util",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./listModels": {
      "types": "./dist/listModels.d.ts",
      "import": "./dist/listModels.js",
      "require": "./dist/listModels.js"
    },
    "./memoryAdapter": {
      "types": "./dist/memoryAdapter.d.ts",
      "import": "./dist/memoryAdapter.js",
      "require": "./dist/memoryAdapter.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js",
      "require": "./dist/logger.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.985.0",
    "@aws-sdk/client-bedrock-agentcore": "^3.985.0",
    "@opentelemetry/api": "^1.9.0",
    "@repo/shared": "*"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
